@model IEnumerable<LearningMaterials>

<div class="container">
    <div class="row">
        @foreach (var learningmaterials in Model)
        {
            <div class="col-12 mb-4">
                <h2 class="card-title h3 text-dark text-uppercase">@learningmaterials.Title</h2>
                @Html.Raw(learningmaterials.Description)
            </div>
        }
    </div>
</div>


@* trzeba dodac properties do Modelu zeby bralo po nazwie dane z bazy i pewnie petle if i else zeby wyciagac odpowiednie rzeczy *@
 